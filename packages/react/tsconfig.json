{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"useUnknownInCatchVariables": false,
		"esModuleInterop": true,
		"declaration": false,
		"resolveJsonModule": true
	},
	"include": [
		"./**/rollup.config.ts"
	]
}
